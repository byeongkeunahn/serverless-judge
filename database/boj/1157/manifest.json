{
    "metadata": {
        "src": "https://www.acmicpc.net/problem/1157"
    },
    "cfg": {
        "time-limit": 1000,
        "memory-limit": 0
    },
    "judge": {
        "type": "standard"
    },
    "solver": {
        "name": "solution.py",
        "language": "python3"
    },
    "tests": [
        {
            "type": "static",
            "auto-discovery": true
        },
        {
            "type": "dynamic",
            "path": "gen_rle.py",
            "language": "python3",
            "argv": ["A", 300, "B", 300]
        },
        {
            "type": "dynamic",
            "path": "gen_rle.py",
            "language": "python3",
            "argv": ["A", 126, "B", 382]
        },
        {
            "type": "dynamic",
            "path": "gen_rle.py",
            "language": "python3",
            "argv": ["A", 255, "B", 127]
        },
        {
            "type": "stress",
            "path": "gen_1.py",
            "language": "python3",
            "max_try_count": 30
        }
    ]
}